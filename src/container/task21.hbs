<main>
    <section class="mb-5">
        <div>
            <div class="card">
                <div class="card-body">
                    {{#with heading}}
                    <div class="mb-3">
                        <div class="card">
                            <div class="card-body">
                                <div>
                                    <div>
                                        {{#with main}}
                                        <div class="display-1 text-dark mb-2">{{title}}</div>
                                    </div>
                                    <div div class="mb-2">
                                        <p>{{text}}</p>
                                    </div>
                                    <div class="mb-2">
                                        <button class="btn btn-primary">
                                            {{#with button_text}}
                                            {{solid}}
                                            {{/with}}
                                        </button>
                                    </div>

                                    <div class="mb-2">
                                        <button class="btn btn-outline-primary">
                                            {{#with button_text}}
                                            {{outline}}
                                            {{/with}}
                                        </button>
                                    </div>
                                    {{/with}}
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="card mb-3">
                        <div class="card-body">
                            <div>
                                <div class="mb-2">
                                    {{#with second}}
                                    <div class="display-2 text-dark">{{title}}</div>
                                </div>
                                <div class="mb-2">
                                    <p>{{text}}</p>
                                </div>
                                <div class="mb-2">
                                    <button class="btn btn-secondary">
                                        {{#with button_text}}
                                        {{solid}}
                                        {{/with}}
                                    </button>
                                </div>

                                <div class="mb-2">
                                    <button class="btn btn-outline-secondary">
                                        {{#with button_text}}
                                        {{outline}}
                                        {{/with}}
                                    </button>
                                </div>
                                {{/with}}
                            </div>
                        </div>
                    </div>

                    <div class="card mb-2">
                        <div class="card-body">
                            <div>

                                {{#with subblock}}
                                <div class="display-3 text-dark mb-2">{{title}}</div>
                                <div class="mb-2">
                                    <p>{{text}}</p>
                                </div>
                                <div class="mb-2">
                                    <button class="btn btn-dark">
                                        {{#with button_text}}
                                        {{solid}}
                                        {{/with}}
                                    </button>
                                </div>

                                <div class="mb-2">
                                    <button class="btn btn-outline-dark">
                                        {{#with button_text}}
                                        {{outline}}
                                        {{/with}}
                                    </button>
                                </div>
                                {{/with}}
                            </div>
                        </div>
                    </div>
                    {{/with}}
                </div>
            </div>
        </div>
    </section>

    <section class="mb-2">
        <div>
            {{#with nested_display}}
            <div class="card">
                <div class="card-body">
                    <div class="display-1 text-dark mb-2">{{title}}</div>
                    <br>
                    <div class="card">
                        <div class="card-body">
                            {{#with children_display}}
                            <div class="display-4 text-dark mb-2">{{title}}</div>

                            <div class="mb-2">
                                <p>{{text}}</p>
                            </div>

                            {{#with button}}
                            <button class="btn btn-outline-warning mb-2">
                                {{text}}
                            </button>
                            {{/with}}
                            {{/with}}
                        </div>
                    </div>

                    <div class="display-6 text-dark mb-2">{{header_text}}</div>

                    {{#with button}}
                    <button class="btn btn-info">
                        {{text}}
                    </button>
                    {{/with}}
                </div>
            </div>
            {{/with}}
        </div>
    </section>


    <section>
        <div>
            {{#with paragraph}}
            <div class="card">
                <div class="card-body">
                    <div class="mb-2"> <b>
                            <p class="text-uppercase"> {{title}}
                            </p>
                        </b></div>

                    <div class="mb-2">
                        <p><b>{{text}}</b></p>
                    </div>
                    <div>
                        {{#with button}}
                        <a href="{{url}}" class="btn btn-link">
                            {{text}}
                        </a>
                        {{/with}}
                    </div>
                    <div class="card">
                        <div class="card-body">
                            {{#each list_name}}
                            <ul>
                                <li>
                                    <a href="{{url}}" class="btn btn-link">
                                        {{text}}
                                    </a>
                                </li>
                            </ul>
                            {{/each}}
                        </div>
                    </div>
                </div>
            </div>
            {{/with}}
        </div>
    </section>
</main>