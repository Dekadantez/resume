<main>
    {{#with web}}
    <div>
        <section>
            <div class="mb-3">{{> title data="WEB"}}</div>
            <hr>
            <div class="mb-3">
                <b><em>LANGUAGE:</em></b>
            </div>
            <ul>
                {{#each languages}}
                <li><b>Name: <mark><abbr>{{name}}</abbr></mark></b></li>
                <p><b>Version:</b> {{version}}</p>
                <p><b>Description:</b> {{description}}</p>
                <br>
                <div class="mb-3">
                    <ul>

                        {{#each elements}}
                        <ol><b>Elements:</b></ol>
                        <ol>Name: <b>{{name}}</b></ol>
                        <ol>Description: {{description}}</ol>

                        <ul>
                            {{#each attributes}}
                            <ol><b>Attributes:</b></ol>

                            <ol>Name: <em>{{name}}</em></ol>
                            <ol>Description: {{description}}</ol>
                            <br>
                            {{/each}}

                        </ul>
                        {{/each}}
                        <div>
                            {{#each properties}}
                            <ol><b>Properties:</b></ol>
                            <ol>Name: <b>{{name}}</b></ol>
                            <ol>Description: {{description}}</ol>
                            <ul>
                                <br>
                                {{#each values}}
                                <ol><b>Values:</b></ol>
                                <ol>Value: <b>{{value}}</b></ol>
                                <ol>Description: <em>{{description}}</em></ol>
                                <br>
                                {{/each}}
                                <br>
                                <br>
                            </ul>
                            {{/each}}
                        </div>
                        <div>
                            {{#each functions}}
                            <ol><b>Function:</b></ol>
                            <ol>Name: <b>{{name}}</b></ol>
                            <ol>Description: {{description}}</ol>
                            <ul>
                                <br>
                                {{#each parameters}}
                                <ol><b>Parameters:</b></ol>
                                <ol>Name: <b>{{name}}</b></ol>
                                <ol>Type: <b>{{type}}</b></ol>
                                <ol>Description: <em>{{description}}</em></ol>
                                <br>
                                {{/each}}
                                <br>
                                <br>
                            </ul>
                            {{/each}}
                        </div>
                    </ul>
                </div>
                {{/each}}
            </ul>
        </section>
    </div>
    {{/with}}
</main>