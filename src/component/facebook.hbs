{{#unless users}}
<li>
    <p><b><abbr title='identifier'>ID</abbr>:</b> {{id}}</p>
</li>
<p><b>name:</b> {{name}}</p>
<p><b>gender:</b> {{gender}}</p>
<p><b>age:</b> {{age}}</p>
<br>
<p><b>Friends:</b>
    <br>
<ul>
    {{#each friends}}
    {{#unless friends}}
    <li>
        <p><b>ID:</b> {{id}}</p>
    </li>

    <p><b>name:</b> {{name}}</p>
    <p><b>gender:</b> {{gender}}</p>
    <p><b>age:</b> {{age}}</p>
    <p><b>mutual friends:</b>
        <br>
    <ul>
        {{#each mutual_friends}}
        <li>
            <p><b>ID:</b> {{id}}</p>
        </li>

        <p><b>name:</b> {{name}}</p>
        <p><b>gender:</b> {{gender}}</p>
        <p><b>age:</b> {{age}}</p>
        {{#each mutual_friends}}
        {{#unless mutual_friends}}
        <p><b>mutual friends:</b>
        <ul>
            <li>
                <p><b>ID:</b> {{id}}</p>
            </li>

            <p><b>name:</b> {{name}}</p>
            <p><b>gender:</b> {{gender}}</p>
            <p><b>age:</b> {{age}}</p>
        </ul>
        <br>
        {{/unless}}
        {{/each}}
        {{/each}}
    </ul>
    </p>

    <ul>
        {{#each groups}}
        {{#unless groups}}
        <p><b>Groups:</b></p>
        <p><b>ID:</b> {{id}}</p>
        <p><b>name:</b> {{name}}</p>
        <p><b>description:</b> {{description}}</p>
        <p><b>members:</b>
            <br>
        <ul>
            {{#each members}}
            <p><b>ID:</b> {{id}}</p>
            <p><b>name:</b> {{name}}</p>
            <p><b>gender:</b> {{gender}}</p>
            <p><b>age:</b> {{age}}</p>
            <br>
            {{/each}}
        </ul>
        </p>
        {{/unless}}
        {{/each}}
    </ul>
    <br>
    <br>
    {{/unless}}
    {{/each}}
</ul>
</p>

<ul>
    {{#each groups}}
    {{#unless groups}}
    <p><b>Groups:</b></p>
    <p><b>ID:</b> {{id}}</p>
    <p><b>name:</b> {{name}}</p>
    <p><b>description:</b> {{description}}</p>
    <p><b>members:</b>
        <br>
    <ul>
        {{#each members}}
        <p><b>ID:</b> {{id}}</p>
        <p><b>name:</b> {{name}}</p>
        <p><b>gender:</b> {{gender}}</p>
        <p><b>age:</b> {{age}}</p>
        <br>
        {{/each}}
    </ul>
    </p>
    {{/unless}}
    {{/each}}
</ul>
{{/unless}}